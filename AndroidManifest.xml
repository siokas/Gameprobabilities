            android:name="com.gameprobabilities.siokas.GeoLocationService"
            android:enabled="true" />

        <activity
            android:name="com.gameprobabilities.siokas.MainActivity"
            android:label="@string/title_activity_main" >
             <intent-filter>
                <action android:name="com.gameprobabilities.siokas.MAIN_ACTIVITY" />

