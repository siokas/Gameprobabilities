            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.gameprobabilities.siokas.JOKER_DRAW" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.gameprobabilities.siokas.Lotto_Draw"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.gameprobabilities.siokas.LOTTO_DRAW" />

                <category android:name="android.intent.category.DEFAULT" />
