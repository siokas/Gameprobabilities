            android:name="com.google.ads.AdActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize" />
        <activity
            android:name="com.gameprobabilities.siokas.Splash"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.gameprobabilities.siokas.Joker_Res"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.gameprobabilities.siokas.JOKER_RES" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        
        <activity
            android:name="com.gameprobabilities.siokas.Lotto_Res"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.gameprobabilities.siokas.LOTTO_RES" />

                <category android:name="android.intent.category.DEFAULT" />
