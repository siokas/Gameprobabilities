            </intent-filter>
        </activity>
        <activity
            android:name="com.gameprobabilities.siokas.GameProbabilitiesMain"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.gameprobabilities.siokas.MENU" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.gameprobabilities.siokas.Joker"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.gameprobabilities.siokas.JOKER" />

                <category android:name="android.intent.category.DEFAULT" />
